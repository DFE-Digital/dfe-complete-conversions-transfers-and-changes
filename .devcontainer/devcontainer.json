{
  "name": "DFE Complete Rails (compose)",
  "dockerComposeFile": [
    "../docker-compose.yml",
    "../docker-compose.dev.yml"
  ],
  "service": "app",
  "runServices": ["app", "js", "sidekiq", "db", "redis", "redis-insight"],
  "workspaceFolder": "/srv/app",
  "remoteUser": "rails",
  "overrideCommand": false,
  "shutdownAction": "stopCompose",
  "forwardPorts": [3000, 5540, 6379, 1433],
  "portsAttributes": {
    "3000": { "label": "Rails", "onAutoForward": "notify" },
    "5540": { "label": "Redis Insight", "onAutoForward": "silent" },
    "6379": { "label": "Redis", "onAutoForward": "silent" },
    "1433": { "label": "SQL Server", "onAutoForward": "silent" }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Shopify.ruby-lsp",
        "rubocop.vscode-rubocop",
        "bung87.rails"
      ]
    }
  }
}
